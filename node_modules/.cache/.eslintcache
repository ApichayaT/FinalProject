lumn":21,"nodeType":"235","messageId":"236","endLine":2,"endColumn":25},{"ruleId":"233","severity":1,"message":"242","line":2,"column":27,"nodeType":"235","messageId":"236","endLine":2,"endColumn":37},{"ruleId":"233","severity":1,"message":"243","line":2,"column":39,"nodeType":"235","messageId":"236","endLine":2,"endColumn":50},{"ruleId":"233","severity":1,"message":"244","line":2,"column":52,"nodeType":"235","messageId":"236","endLine":2,"endColumn":58},{"ruleId":"233","severity":1,"message":"245","line":2,"column":60,"nodeType":"235","messageId":"236","endLine":2,"endColumn":69},{"ruleId":"233","severity":1,"message":"246","line":2,"column":71,"nodeType":"235","messageId":"236","endLine":2,"endColumn":80},{"ruleId":"233","severity":1,"message":"247","line":2,"column":82,"nodeType":"235","messageId":"236","endLine":2,"endColumn":90},{"ruleId":"233","severity":1,"message":"248","line":2,"column":92,"nodeType":"235","messageId":"236","endLine":2,"endColumn":96},"no-native-reassign",["249"],"no-negated-in-lhs",["250"],["249"],["250"],"no-unused-vars","'homeObjThree' is defined but never used.","Identifier","unusedVar","'HeroBtnWrapper' is defined but never used.","'useState' is defined but never used.","'ContentConclusion' is defined but never used.","'Container' is defined but never used.","'Form' is defined but never used.","'FormButton' is defined but never used.","'FormContent' is defined but never used.","'FormH1' is defined but never used.","'FormInput' is defined but never used.","'FormLabel' is defined but never used.","'FormWrap' is defined but never used.","'Text' is defined but never used.","no-global-assign","no-unsafe-negation"]